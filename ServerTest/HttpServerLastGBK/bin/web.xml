<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<servlet>
	<servlet-name>LoginServlet</servlet-name>
	<servlet-class>com.wx.user.LoginServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>RegisterServlet</servlet-name>
	<servlet-class>com.wx.user.RegisterServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>OtherServlet</servlet-name>
	<servlet-class>com.wx.user.OtherServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>LoginServlet</servlet-name>
	<url-pattern>/login</url-pattern>
	<url-pattern>/g</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>RegisterServlet</servlet-name>
	<url-pattern>/regist</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>OtherServlet</servlet-name>
	<url-pattern>/oth</url-pattern>
</servlet-mapping>
</web-app>